{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "no-undef":"off",
        "class-methods-use-this":"off",
        "no-underscore-dangle":"off",
        "no-unused-vars":"off"
    }
}
